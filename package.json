{
  "name": "porto-reactjs",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "del": "rm -rf ../ray123fa/assets && rm -rf ../ray123fa/img && rm ../ray123fa/index.html",
    "cphtaccess": "cp -r .htaccess/.htaccess dist/assets && cp -r .htaccess/.htaccess dist/img",
    "cpdist": "cp -r dist/assets ../ray123fa && cp -r dist/img ../ray123fa && cp dist/index.html ../ray123fa",
    "build": "npm run del && npm run cphtaccess && npm run cpdist",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "aos": "3.0.0-beta.6",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prop-types": "^15.8.1",
    "vite": "^5.3.3"
